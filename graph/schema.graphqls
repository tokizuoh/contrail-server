scalar Date

enum WorkoutType {
  cycling
  running
}

type Workout {
  id: ID!
  distance: Float!
  duration: Float!
  startDate: Date!
  type: WorkoutType!
}

type Query {
  workouts: [Workout!]!
}

input WorkoutInput {
  distance: Float!
  duration: Float!
  startDate: Date!
  type: WorkoutType!
}

type Mutation {
  createWorkout(input: WorkoutInput!): Workout!
}