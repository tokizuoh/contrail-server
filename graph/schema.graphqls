interface Node {
  id: ID!
}

type Query {
  workouts: [Workout!]
  node(id: ID!): Node
}

type Mutation {
  createUser(input: UserInput!): User!
}

type Workout implements Node {
  id: ID!
  distance: Float!
}

type User implements Node{
  id: ID!
  name: String!
  workouts: [Workout!]!
}

input WorkoutInput {
  distance: Float!
}

input UserInput {
  name: String!
  workouts: [WorkoutInput!]!
}