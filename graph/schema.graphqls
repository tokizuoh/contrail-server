type Workout {
  distance: Float!
  duration: Float!
  startDate: Float!
  endDate: Float!
  testNumber: Float!
}

type Query {
  workouts: [Workout!]!
}

input NewWorkout {
  distance: Float!
  duration: Float!
  startDate: Float!
  endDate: Float!
}

type Mutation {
  createWorkout(input: NewWorkout!): Workout!
}