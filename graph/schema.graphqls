type Query {
  workouts: [Workout!]
  workout(id: ID!): Workout
  user(id: ID!): User
}

type Mutation {
  createUser(input: UserInput!): User!
}

type Workout {
  id: ID!
  distance: Float!
}

type User {
  id: ID!
  name: String!
  workouts: [Workout!]!
}

input WorkoutInput {
  distance: Float!
}

input UserInput {
  name: String!
  workouts: [WorkoutInput!]!
}